{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
  },
  "variables": {
    "DsVnPrefix": "10.0.0.0/16",
    "DsVnSubnet1Name": "FeSn",
    "DsVnSubnet1Prefix": "10.0.0.0/24",
    "DsVnSubnet2Name": "SemSn",
    "DsVnSubnet2Prefix": "10.0.1.0/24",
    "DsVnSubnet3Name": "BeSn",
    "DsVnSubnet3Prefix": "10.0.2.0/24"
  },
  "resources": [
    {
      "name": "DsVn",
      "type": "Microsoft.Network/virtualNetworks",
      "location": "[resourceGroup().location]",
      "apiVersion": "2015-06-15",
      "dependsOn": [ ],
      "tags": {
        "displayName": "DsVn"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('DsVnPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[variables('DsVnSubnet1Name')]",
            "properties": {
              "addressPrefix": "[variables('DsVnSubnet1Prefix')]"
            }
          },
          {
            "name": "[variables('DsVnSubnet2Name')]",
            "properties": {
              "addressPrefix": "[variables('DsVnSubnet2Prefix')]"
            }
          },
          {
            "name": "[variables('DsVnSubnet3Name')]",
            "properties": {
              "addressPrefix": "[variables('DsVnSubnet3Prefix')]"
            }
          }
        ]
      }
    }
  ],
  "outputs": {
  }
}
